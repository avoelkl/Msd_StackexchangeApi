<h1>Registered Users</h1>
<?php
$collection = $this->getAuthenticatedUserList();
?>
<ul>
<?php foreach($collection as $user): ?>
<li>
    <img src="<?php echo $user->getProfileImage(); ?>" height="40" style="display: inline; vertical-align: middle;"/>
    <a href="<?php $user->getLink(); ?>"><?php echo $user->getDisplayName(); ?></a>
</li>
<?php endforeach; ?>
</ul>